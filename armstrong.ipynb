    "Armstrong number"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "3137942b-782c-417b-89ba-f6d7f3c2aabf",
   "metadata": {},
   "outputs": [
    {
     "name": "stdin",
     "output_type": "stream",
     "text": [
      "Enter a number:  524\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "524 is NOT an Armstrong number\n"
     ]
    }
   ],
   "source": [
    "num = int(input(\"Enter a number: \"))\n",
    "digits = str(num)\n",
    "power = len(digits)\n",
    "total = sum(int(digit) ** power for digit in digits)\n",
    "\n",
    "if num == total:\n",
    "    print(num, \"is an Armstrong number\")\n",
    "else:\n",
    "    print(num, \"is NOT an Armstrong number\")\n"
   ]
  },
